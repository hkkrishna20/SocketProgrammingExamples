<jaxws:bindings xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	wsdlLocation="../wsdl/AWSECommerceService.wsdl" xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	xmlns:javaee="http://java.sun.com/xml/ns/javaee" version="2.0">
	<jaxws:bindings node="wsdl:definitions">
		<!-- The handler-chains tag must be present else wsimport silently ignores 
			the handlers -->
		<javaee:handler-chains>
			<javaee:handler-chain>
				<javaee:handler>
					<javaee:handler-name>SOAPMessageLoggingHandler
					</javaee:handler-name>
					<javaee:handler-class>
						name.javacode.webservices.jaxws.handler.SOAPMessageLoggingHandler
					</javaee:handler-class>
				</javaee:handler>
				<javaee:handler>
					<javaee:handler-name>RequestSignatureHandler</javaee:handler-name>
					<javaee:handler-class>
						name.javacode.webservices.jaxws.aws.client.RequestSignatureHandler
					</javaee:handler-class>
				</javaee:handler>
			</javaee:handler-chain>
		</javaee:handler-chains>
	</jaxws:bindings>
</jaxws:bindings>