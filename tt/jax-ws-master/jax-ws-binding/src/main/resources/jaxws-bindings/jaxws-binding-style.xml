<jaxws:bindings xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	wsdlLocation="../wsdl/calculator-cf.wsdl" xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	xmlns:javaee="http://java.sun.com/xml/ns/javaee" version="2.0">
	<jaxws:bindings node="wsdl:definitions">
		<!-- It seems that a 'bare style' in the WSDL can't be overridden to make 
			it 'wrapped'; the converse it not true though. A 'wrapped' style in WSDL 
			can be overridden to make it 'bare' -->
		<jaxws:enableWrapperStyle>true</jaxws:enableWrapperStyle>
	</jaxws:bindings>
</jaxws:bindings>